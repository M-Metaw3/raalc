{
  "name": "raalc",
  "version": "1.0.0",
  "description": "RAALC Backend API - Node.js/Express/TypeORM",
  "main": "src/app.js",
  "scripts": {
    "start": "node -r module-alias/register src/app.js",
    "dev": "nodemon -r module-alias/register src/app.js",
    "test": "jest",
    "test:otp": "node tests/otp-flow.test.js",
    "test:admin": "node -r module-alias/register tests/admin-flow.test.js",
    "test:shift": "node -r module-alias/register tests/shift-flow.test.js",
    "test:agent": "node -r module-alias/register tests/agent-crud.test.js",
    "test:postman": "powershell -ExecutionPolicy Bypass -File scripts/test-admin-postman.ps1",
    "migrate": "node scripts/migrate.js",
    "migrate:create": "node scripts/createMigration.js",
    "seed": "node -r module-alias/register src/seeders/index.js",
    "seed:rollback": "node -r module-alias/register src/seeders/index.js rollback"
  },
  "_moduleAliases": {
    "@config": "src/config",
    "@constants": "src/constants",
    "@controllers": "src/controllers",
    "@middleware": "src/middleware",
    "@models": "src/models",
    "@repositories": "src/repositories",
    "@routes": "src/routes",
    "@services": "src/services",
    "@utils": "src/utils",
    "@validators": "src/validators",
    "@seeders": "src/seeders"
  },
  "keywords": [
    "nodejs",
    "express",
    "typeorm",
    "mysql",
    "api"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.12.2",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-rate-limit": "^6.7.0",
    "express-validator": "^7.0.1",
    "form-data": "^4.0.4",
    "helmet": "^7.0.0",
    "i18next": "^22.4.15",
    "i18next-fs-backend": "^2.1.1",
    "i18next-http-middleware": "^3.3.0",
    "jsonwebtoken": "^9.0.0",
    "module-alias": "^2.2.3",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.2.4",
    "redis": "^4.6.5",
    "typeorm": "^0.3.12",
    "winston": "^3.8.2",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "jest": "^29.5.0",
    "nodemon": "^2.0.22",
    "supertest": "^6.3.3"
  }
}
